

<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>

        <%@include file="estrutura/importBot.jsp"%>
        <link href="<%=request.getContextPath()%>/resources/css/cadastrar.css" rel="stylesheet">
        <title>Cadastrar</title>
    </head>
    <body>
        <%@include file="estrutura/cabecalho.jsp"%>

        <form:form action="salvarUsuario" id="" commandName="TbCliente" method="POST">

            <!--form:input type="hidden" path="senhacli" cssClass="form-control" id="senha" value="1234"/-->
            <!-- form start -->

            <div id="wrapper">
                <div class="container body-content">
            <div class="row justify-content-center">
                <div class="col-8">
                    <div class="form-group">
                        <form:label path="nomecli" for="nome">${formNome}</form:label>
                        <form:input id="nome" type="text" path="nomecli" cssClass="form-control" placeholder="nome" required="required" data-tootle="tooltip"></form:input>
                        </div>
                    </div>


                </div>

                <div class="row justify-content-center">
                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="cpf" for="cpf">${formCpf}</form:label>
                        <form:input class="cpf" id="cpf" type="text" path="cpf" cssClass="form-control" placeholder="cpf" required="required" data-tootle="tooltip" value=""></form:input>
                        </div>
                    </div>

                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="emailcli" for="email">${formEmail}</form:label>
                        <form:input id="email" type="text" path="emailcli" cssClass="form-control" placeholder="exemplo@gmail.com" required="required" data-tootle="tooltip" value=""></form:input>
                        </div>
                    </div>     
                </div>

                <div class="row justify-content-center">
                    <div class="col-8">
                        <div class="form-group">
                        <form:label path="senhacli" for="senha">${formSenha}</form:label>
                        <form:input id="senha" type="password" path="senhacli" cssClass="form-control" placeholder="password" required="required" data-tootle="tooltip"></form:input>
                        </div>
                    </div>            
                </div>


                <div class="row justify-content-center">
                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="ruacli" for="rua">${formRua}</form:label>
                        <form:input id="rua" type="text" path="ruacli" cssClass="form-control" placeholder="rua" required="required" data-tootle="tooltip"></form:input>
                        </div>
                    </div>        



                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="numerocli" for="numero">${formNumero}</form:label>
                        <form:input id="numero" type="text" path="numerocli" cssClass="form-control" placeholder="numero" required="required" data-tootle="tooltip"></form:input>
                        </div>
                    </div>            
                </div>

                <div class="row justify-content-center">
                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="bairrocli" for="bairro">${formBairro}</form:label>
                        <form:input id="bairro" type="text" path="bairrocli" cssClass="form-control" placeholder="bairro" required="required" data-tootle="tooltip"></form:input>
                        </div>

                    </div>



                    <div class="col-4">
                        <div class="form-group">
                        <form:label path="cidadecli" for="cidade">${formCidade}</form:label>
                        <form:input id="cidade" type="text" path="cidadecli" cssClass="form-control" placeholder="cidade" required="required" data-tootle="tooltip"></form:input>
                        </div>
                    </div>  
                </div>


                <div class="row justify-content-center">
                    <div class="col-3">
                        <div class="form-group">
                        <form:label path="estadocli" for="estado">${formEstado}</form:label>
                        <form:select id="estado" path="estadocli" cssClas="form-control" required="requeired" data-tootle="tooltip">
                            <form:option value="AC">AC</form:option>
                            <form:option value="AL">AL</form:option>
                            <form:option value="AP">AP</form:option>
                            <form:option value="AM">AM</form:option>
                            <form:option value="BA">BA</form:option>
                            <form:option value="CE">CE</form:option>
                            <form:option value="DF">DF</form:option>
                            <form:option value="ES">ES</form:option>
                            <form:option value="GO">GO</form:option>
                            <form:option value="MA">MA</form:option>
                            <form:option value="MT">MT</form:option>
                            <form:option value="MS">MS</form:option>
                            <form:option value="MG">MG</form:option>
                            <form:option value="PA">PA</form:option>
                            <form:option value="PB">PB</form:option>
                            <form:option value="PR">PR</form:option>
                            <form:option value="PE">PE</form:option>
                            <form:option value="PI">PI</form:option>
                            <form:option value="RJ">RJ</form:option>
                            <form:option value="RN">RN</form:option>
                            <form:option value="RS">RS</form:option>
                            <form:option value="RO">RO</form:option>
                            <form:option value="RR">RR</form:option>
                            <form:option value="SC">SC</form:option>
                            <form:option value="SP">SP</form:option>
                            <form:option value="SE">SE</form:option>
                            <form:option value="TO">TO</form:option>
                        </form:select>
                    </div>
                </div>            
            </div>


            <div class="row justify-content-center">
                <div class="col-4">
                    <input type="submit" value="Cadastar">
                </div>
                <div class="col-4">
                    <input type="reset" value="Limpar">
                </div>
            </div>
                </div>
                        <%@include file="estrutura/rodape.jsp"%> 
            </div>



        </form:form>
        
    </body>
      
</html>


